#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rahul/Desktop/code/ollama-flow/node_modules/.pnpm/vinxi@0.4.3_@types+node@22.7.8_ioredis@5.4.1_terser@5.36.0/node_modules/vinxi/bin/node_modules:/Users/rahul/Desktop/code/ollama-flow/node_modules/.pnpm/vinxi@0.4.3_@types+node@22.7.8_ioredis@5.4.1_terser@5.36.0/node_modules/vinxi/node_modules:/Users/rahul/Desktop/code/ollama-flow/node_modules/.pnpm/vinxi@0.4.3_@types+node@22.7.8_ioredis@5.4.1_terser@5.36.0/node_modules:/Users/rahul/Desktop/code/ollama-flow/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rahul/Desktop/code/ollama-flow/node_modules/.pnpm/vinxi@0.4.3_@types+node@22.7.8_ioredis@5.4.1_terser@5.36.0/node_modules/vinxi/bin/node_modules:/Users/rahul/Desktop/code/ollama-flow/node_modules/.pnpm/vinxi@0.4.3_@types+node@22.7.8_ioredis@5.4.1_terser@5.36.0/node_modules/vinxi/node_modules:/Users/rahul/Desktop/code/ollama-flow/node_modules/.pnpm/vinxi@0.4.3_@types+node@22.7.8_ioredis@5.4.1_terser@5.36.0/node_modules:/Users/rahul/Desktop/code/ollama-flow/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vinxi@0.4.3_@types+node@22.7.8_ioredis@5.4.1_terser@5.36.0/node_modules/vinxi/bin/cli.mjs" "$@"
else
  exec node  "$basedir/../../../../../../vinxi@0.4.3_@types+node@22.7.8_ioredis@5.4.1_terser@5.36.0/node_modules/vinxi/bin/cli.mjs" "$@"
fi
